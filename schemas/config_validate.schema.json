{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "NEON config validate schema",
  "type": "object",
  "required": [
    "file",
    "ok",
    "error",
    "keys"
  ],
  "properties": {
    "file": {
      "type": [
        "string",
        "null"
      ]
    },
    "ok": {
      "type": "boolean"
    },
    "error": {
      "type": [
        "string",
        "null"
      ]
    },
    "keys": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": true
}